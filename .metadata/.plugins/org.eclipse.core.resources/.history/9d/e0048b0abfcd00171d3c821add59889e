                function check()
                {
                title=document.getElementById("title");
                phone=document.getElementById("phone");
                if (title.value=="")
                    {
                        alert("提示：请输入您的姓名！")
                        title.focus()
                        return false
                    }
                if (phone.value=="")
                    {
                        alert("提示：请输入您的联系电话！")
                        phone.focus()
                        return false
                    }
                }
